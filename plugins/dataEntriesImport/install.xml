        <update version="1.2.2" files="rlDataEntriesImport.class.php,admin/dataEntriesImport.inc.php,admin/dataEntriesImport.tpl,admin/static/.htaccess,admin/static/sample-txt.jpg,admin/static/sample-xls.jpg,admin/static/import-csv.jpg,i18n/ru.json"><![CDATA[]]></update>
